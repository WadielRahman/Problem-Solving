class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i in range(len(nums)):
            for j in range(1, len(nums)):
                sum = nums[i] + nums[j]
                if sum == target and i != j:
                    return [i, j]
                    break
Sol = Solution()
print(Sol.twoSum([2,7,11,15], 9))
            
        
